import subprocess

rule process_run:
    run:
         process_call = ['/usr/local/bin/DRAGEN_TruSight_Oncology_500_ctDNA.sh','--version']
         try:
             subprocess.run(process_call).check_returncode()
             print(subprocess.run(process_call).stdout)
         except subprocess.CalledProcessError as e:
             print(f"Process failed with a return code: {e.returncode}.")
             # message = f"Process failed with a return code: {e.returncode}."
             # raise RuntimeError(message)

         # process_call = ['whoami']
         # try:
         #     subprocess.run(process_call).check_returncode()
